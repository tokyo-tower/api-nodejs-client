{
  "name": "@motionpicture/ttts-api-nodejs-client",
  "version": "9.3.0",
  "description": "TTTS API client library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./doc ./coverage ./.nyc_output",
    "example": "http-server ./example",
    "doc": "echo \"implementing...\" && exit 0",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc && nyc report --reporter=text-lcov | coveralls",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tokyo-tower/api-nodejs-client.git"
  },
  "keywords": [
    "ttts"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.5.0",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^2.2.48",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.10.61",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.5.3",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^9.0.4",
    "coveralls": "^2.13.3",
    "http-server": "^0.10.0",
    "mocha": "^3.5.3",
    "moment": "^2.26.0",
    "nock": "^9.6.1",
    "nyc": "^11.9.0",
    "open": "6.0.0",
    "power-assert": "^1.6.1",
    "request": "^2.88.2",
    "rimraf": "^2.7.1",
    "sinon": "^6.3.5",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@motionpicture/ttts-api-abstract-client": "9.3.0",
    "debug": "^3.2.6",
    "http-status": "^1.4.2",
    "isomorphic-fetch": "^2.2.1"
  }
}
